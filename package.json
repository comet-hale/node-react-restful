{
  "name": "express_react",
  "version": "1.0.0",
  "description": "Basic Express React Boilerplate",
  "engines": {
    "node": ">=7.5.0"
  },
  "scripts": {
    "start": "nodemon index.js",
    "build": "npm run build_server && npm run build_client && node assets/server.js",
    "build_server": "webpack -p --config webpack.prod_server.config.js ",
    "build_client": "webpack -p --config webpack.prod_client.config.js",
    "pack": "webpack --config webpack.dev.config.js",
    "dbset": "sequelize db:migrate && sequelize db:seed:all"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.2",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.3.0",
    "multer": "^1.4.1",
    "mysql2": "^1.6.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-file-download": "^0.3.5",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^3.11.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "ISC"
}
